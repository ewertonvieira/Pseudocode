algoritmo "Q_14"
// Função :
// Autor :
// Data : 12/05/2024
// Seção de Declarações
var
a, b, aux : Real
inicio
// Seção de Comandos
Escreva("Informe o valor de A: ")
Leia(a)
Escreva("Informe B: ")
Leia(b)

// Trocando valores utilizando uma variavel auxiliar
aux <- a
a <- b
b <- aux

Escreval("valor de A:",a," Valor de B:", b)
fimalgoritmo
